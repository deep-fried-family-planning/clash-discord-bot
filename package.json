{
  "name": "clash-discord-bot",
  "version": "1.0.0",
  "description": "",
  "author": "",
  "license": "ISC",
  "engineStrict": true,
  "engines": {
    "node": "20.12.2"
  },
  "packageManager": "pnpm@9.12.0",
  "type": "module",
  "scripts": {
    "types": "tsc --project tsconfig.check.json --noEmit",
    "build": "rm -rf dist && rspack build",
    "coverage": "vitest --coverage",
    "lint": "eslint . --quiet",
    "fixlint": "eslint . --fix",
    "unit": "echo 'not implemented'",
    "format": "terraform -chdir=terraform fmt -recursive",
    "init": "terraform -chdir=terraform init",
    "select": "terraform -chdir=terraform workspace select",
    "show": "terraform -chdir=terraform workspace show",
    "plan": "pnpm run show && terraform -chdir=terraform plan -out .plan.tfplan",
    "apply": "terraform -chdir=terraform apply .plan.tfplan",
    "reset": "rm -rf **/node_modules **/dist **/*.tfplan **/*.lock.hcl **/.terraform",
    "sendit": "pnpm run build && pnpm run plan && pnpm run apply",
    "commitlint": "commitlint --edit",
    "prepare": "husky"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "3.667.0",
    "@aws-sdk/client-sqs": "3.665.0",
    "@aws-sdk/lib-dynamodb": "3.665.0",
    "@discordjs/builders": "1.9.0",
    "@discordjs/core": "2.0.0",
    "@discordjs/rest": "2.4.0",
    "@effect-aws/client-dynamodb": "1.4.0",
    "@effect-aws/client-eventbridge": "1.5.0",
    "@effect-aws/client-lambda": "1.5.0",
    "@effect-aws/client-sqs": "1.5.0",
    "@effect-aws/client-ssm": "1.1.0",
    "@effect-aws/lambda": "1.2.0",
    "@effect-aws/lib-dynamodb": "1.3.0",
    "@effect-aws/powertools-logger": "1.3.0",
    "@effect-aws/ssm": "1.0.1",
    "@hapi/boom": "10.0.1",
    "@hapi/bourne": "3.0.0",
    "bufferutil": "4.0.8",
    "clashofclans.js": "3.3.13",
    "dayjs": "1.11.13",
    "discord-interactions": "4.1.0",
    "effect": "3.9.1",
    "joi": "17.13.3",
    "natural": "8.0.1",
    "simple-statistics": "7.8.5",
    "string-similarity": "4.0.4",
    "uuid": "10.0.0"
  },
  "devDependencies": {
    "@aws-sdk/client-ssm": "3.665.0",
    "@commitlint/cli": "19.5.0",
    "@commitlint/config-conventional": "19.5.0",
    "@eslint/js": "9.12.0",
    "@rspack/cli": "1.0.8",
    "@rspack/core": "1.0.10",
    "@stylistic/eslint-plugin": "2.9.0",
    "@types/aws-lambda": "8.10.145",
    "@types/eslint__js": "8.42.3",
    "@types/node": "22.7.4",
    "@types/string-similarity": "4.0.2",
    "@types/uuid": "10.0.0",
    "@vitest/coverage-istanbul": "2.1.2",
    "eslint": "9.12.0",
    "husky": "9.1.6",
    "rspack-chain": "1.0.3",
    "typescript": "5.6.2",
    "typescript-eslint": "8.8.0",
    "vite-tsconfig-paths": "5.0.1",
    "vitest": "2.1.2"
  }
}
