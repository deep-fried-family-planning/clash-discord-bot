{
  "name": "clash-discord-bot",
  "version": "1.0.0",
  "description": "",
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.11.0",
  "engineStrict": true,
  "engines": {
    "node": "22.15.0"
  },
  "type": "module",
  "scripts": {
    "pnpmv": "pnpm -v",
    "bench": "vitest bench",
    "lsp": "typescript-language-server --stdio",
    "merge": "pnpm i --merge-git-branch-lockfiles",
    "proxy": "tsx dev/proxy-server.ts",
    "dev": "nodemon --exec tsx dev/dev-client.ts",
    "devrun": "tsx dev/run.ts",
    "types": "tsc --project tsconfig.check.json --noEmit",
    "build": "NODE_OPTIONS='--experimental-transform-types' rspack build",
    "build:doc": "NODE_OPTIONS='--experimental-transform-types' rspack build --config scripts/rspack.config.doctor.mjs",
    "build:dev": "NODE_OPTIONS='--experimental-transform-types' rspack build --config scripts/rspack.config.dev.mjs",
    "build:dev:doc": "NODE_OPTIONS='--experimental-transform-types' rspack build --config scripts/rspack.config.dev.doctor.mjs",
    "coverage": "vitest --coverage",
    "lint": "eslint . --quiet",
    "fixlint": "eslint . --fix",
    "unit": "vitest test/unit",
    "format": "terraform -chdir=terraform fmt -recursive",
    "init": "terraform -chdir=terraform init",
    "select": "terraform -chdir=terraform workspace select",
    "show": "terraform -chdir=terraform workspace show",
    "plan": "pnpm run show && terraform -chdir=terraform plan -var-file .secret.tfvars -out .plan.tfplan",
    "apply": "terraform -chdir=terraform apply .plan.tfplan",
    "reset": "rm -rf **/node_modules **/dist **/*.tfplan **/*.lock.hcl **/.terraform",
    "sendit": "pnpm run build && pnpm run plan && pnpm run apply",
    "teehee": "export BUILD_ENV=$(terraform -chdir=terraform workspace show) && pnpm run format && pnpm run types && pnpm run sendit",
    "commitlint": "commitlint --edit",
    "slash": "tsx scripts/slash.ts",
    "autogen": "pnpm exec swagger-typescript-api generate -o ./autogen --name v1.ts --type-prefix Clash --modular -p ./autogen/v1.yaml",
    "prepare": "husky"
  },
  "dependencies": {
    "@effect-aws/client-api-gateway-management-api": "1.10.3",
    "@effect-aws/client-dynamodb": "1.10.3",
    "@effect-aws/client-eventbridge": "1.10.3",
    "@effect-aws/client-lambda": "1.10.3",
    "@effect-aws/client-scheduler": "1.10.3",
    "@effect-aws/client-sqs": "1.10.3",
    "@effect-aws/client-ssm": "1.10.3",
    "@effect-aws/lambda": "1.5.0",
    "@effect-aws/lib-dynamodb": "1.10.3",
    "@effect-aws/ssm": "1.3.3",
    "@effect/platform": "0.82.3",
    "@effect/platform-node": "0.81.0",
    "@msgpack/msgpack": "3.1.1",
    "@typed/id": "0.17.2",
    "clashofclans.js": "3.3.22",
    "dayjs": "1.11.13",
    "dfx": "0.118.17",
    "discord-verify": "1.2.0",
    "effect": "3.15.2",
    "pako": "2.1.0",
    "regexparam": "3.0.0",
    "simple-statistics": "7.8.8",
    "string-similarity": "4.0.4",
    "uuid": "11.1.0"
  },
  "devDependencies": {
    "@aws-sdk/client-scheduler": "3.812.0",
    "@aws-sdk/lib-dynamodb": "3.812.0",
    "@changesets/cli": "2.29.4",
    "@commitlint/cli": "19.8.1",
    "@commitlint/config-conventional": "19.8.1",
    "@discordjs/core": "2.1.0",
    "@effect/language-service": "0.16.3",
    "@effect/vitest": "0.22.2",
    "@eslint/js": "9.27.0",
    "@rsdoctor/cli": "1.1.2",
    "@rsdoctor/rspack-plugin": "1.1.2",
    "@rspack/cli": "1.3.10",
    "@rspack/core": "1.3.10",
    "@stylistic/eslint-plugin": "4.2.0",
    "@tim-smart/openapi-gen": "0.4.4",
    "@types/aws-lambda": "8.10.149",
    "@types/pako": "2.0.3",
    "@types/string-similarity": "4.0.2",
    "@types/uuid": "10.0.0",
    "@vitest/coverage-v8": "3.1.4",
    "esbuild": "0.25.4",
    "eslint": "9.27.0",
    "husky": "9.1.7",
    "nodemon": "3.1.10",
    "openapi-typescript": "7.8.0",
    "safe-json-stringify": "1.2.0",
    "swagger-typescript-api": "13.1.3",
    "tsoa": "6.6.0",
    "tsx": "4.19.4",
    "typescript": "5.8.3",
    "typescript-eslint": "8.32.1",
    "typescript-language-server": "4.3.4",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.1.4"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "esbuild",
      "msgpackr-extract"
    ]
  }
}
