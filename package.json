{
  "name": "clash-discord-bot",
  "version": "1.0.0",
  "description": "",
  "author": "",
  "license": "ISC",
  "type": "module",
  "engineStrict": true,
  "engines": {
    "node": "22.14.0",
    "pnpm": "10.6.2"
  },
  "packageManager": "pnpm@10.6.2",
  "scripts": {
    "merge": "pnpm i --merge-git-branch-lockfiles",
    "proxy": "tsx dev/ws-proxy.ts",
    "dev": "nodemon --exec tsx dev/ws-client.ts",
    "types": "tsc --project tsconfig.check.json --noEmit",
    "build": "rspack build",
    "build:doc": "rspack build --config rspack.config.doctor.mjs",
    "build:dev": "rspack build --config rspack.config.dev.mjs",
    "build:dev:doc": "rspack build --config rspack.config.dev.doctor.mjs",
    "coverage": "vitest --coverage",
    "lint": "eslint . --quiet",
    "fixlint": "eslint . --fix",
    "unit": "vitest test/unit",
    "format": "terraform -chdir=terraform fmt -recursive",
    "init": "terraform -chdir=terraform init",
    "select": "terraform -chdir=terraform workspace select",
    "show": "terraform -chdir=terraform workspace show",
    "plan": "pnpm run show && terraform -chdir=terraform plan -var-file .secret.tfvars -out .plan.tfplan",
    "apply": "terraform -chdir=terraform apply .plan.tfplan",
    "reset": "rm -rf **/node_modules **/dist **/*.tfplan **/*.lock.hcl **/.terraform",
    "sendit": "pnpm run build && pnpm run plan && pnpm run apply",
    "teehee": "export BUILD_ENV=$(terraform -chdir=terraform workspace show) && pnpm run format && pnpm run types && pnpm run sendit",
    "commitlint": "commitlint --edit",
    "slash": "tsx scripts/slash.ts",
    "prepare": "husky"
  },
  "dependencies": {
    "@effect-aws/client-api-gateway-management-api": "1.9.3",
    "@effect-aws/client-dynamodb": "1.9.3",
    "@effect-aws/client-eventbridge": "1.9.3",
    "@effect-aws/client-lambda": "1.9.3",
    "@effect-aws/client-scheduler": "1.9.3",
    "@effect-aws/client-sqs": "1.9.3",
    "@effect-aws/client-ssm": "1.9.3",
    "@effect-aws/lambda": "1.3.1",
    "@effect-aws/lib-dynamodb": "1.9.3",
    "@effect-aws/ssm": "1.1.1",
    "@effect/platform": "0.79.1",
    "@effect/platform-node": "0.75.1",
    "@hapi/boom": "10.0.1",
    "@hapi/bourne": "3.0.0",
    "@msgpack/msgpack": "3.1.1",
    "@tim-smart/openapi-gen": "0.3.11",
    "brotli": "1.3.3",
    "clashofclans.js": "3.3.21",
    "dayjs": "1.11.13",
    "dfx": "0.114.0",
    "discord-interactions": "4.1.1",
    "effect": "3.13.10",
    "joi": "17.13.3",
    "openapi-typescript": "7.6.1",
    "pako": "2.1.0",
    "regexparam": "3.0.0",
    "simple-statistics": "7.8.8",
    "string-similarity": "4.0.4",
    "tsoa": "6.6.0",
    "uuid": "11.1.0"
  },
  "devDependencies": {
    "@changesets/cli": "2.28.1",
    "@commitlint/cli": "19.8.0",
    "@commitlint/config-conventional": "19.8.0",
    "@discordjs/core": "2.0.1",
    "@effect/vitest": "0.19.8",
    "@eslint/js": "9.22.0",
    "@hapi/hapi": "21.4.0",
    "@hapi/nes": "14.0.1",
    "@rsdoctor/cli": "0.4.13",
    "@rsdoctor/rspack-plugin": "0.4.13",
    "@rspack/cli": "1.2.8",
    "@rspack/core": "1.2.8",
    "@stylistic/eslint-plugin": "4.2.0",
    "@types/aws-lambda": "8.10.147",
    "@types/brotli": "1.3.4",
    "@types/pako": "2.0.3",
    "@types/string-similarity": "4.0.2",
    "@types/uuid": "10.0.0",
    "@types/ws": "8.18.0",
    "@vitest/coverage-istanbul": "3.0.8",
    "esbuild": "0.25.1",
    "esbuild-loader": "4.3.0",
    "esbuild-register": "3.6.0",
    "eslint": "9.22.0",
    "husky": "9.1.7",
    "nodemon": "3.1.9",
    "safe-json-stringify": "1.2.0",
    "terser-webpack-plugin": "5.3.14",
    "ts-node": "10.9.2",
    "tsx": "4.19.3",
    "typescript": "5.8.2",
    "typescript-eslint": "8.26.1",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.0.8",
    "ws": "8.18.1"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild",
      "@parcel/watcher"
    ]
  }
}
