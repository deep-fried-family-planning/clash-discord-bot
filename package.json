{
    "name": "clash-discord-bot",
    "version": "1.0.0",
    "description": "",
    "author": "",
    "license": "ISC",
    "engines": {
        "node": "20.12.2",
        "pnpm": "9.9.0"
    },
    "type": "module",
    "scripts": {
        "types": "tsc --project tsconfig.check.json --noEmit",
        "build": "rm -rf dist && dffp build",
        "coverage": "vitest --coverage",
        "lint": "eslint .",
        "fixlint": "eslint . --fix",
        "format": "terraform -chdir=terraform fmt -recursive",
        "init": "terraform -chdir=terraform init",
        "plan": "terraform -chdir=terraform plan -out .plan.tfplan",
        "apply": "terraform -chdir=terraform apply .plan.tfplan",
        "reset": "rm -rf **/node_modules **/dist **/*.tfplan **/*.lock.hcl **/.terraform",
        "sendit": "pnpm run build && pnpm run plan && pnpm run apply",
        "postinstall": "husky"
    },
    "dependencies": {
        "@aws-sdk/client-dynamodb": "3.637.0",
        "@aws-sdk/client-sqs": "3.637.0",
        "@aws-sdk/lib-dynamodb": "3.637.0",
        "@discordjs/core": "2.0.0",
        "@discordjs/rest": "2.4.0",
        "@hapi/boom": "10.0.1",
        "@hapi/bourne": "3.0.0",
        "clashofclans.js": "3.3.12",
        "day": "0.0.2",
        "discord-interactions": "4.1.0",
        "fp-ts": "2.16.9",
        "joi": "17.13.3",
        "natural": "8.0.1",
        "simple-statistics": "7.8.5",
        "string-similarity": "4.0.4",
        "uuid": "10.0.0"
    },
    "devDependencies": {
        "@aws-sdk/client-ssm": "3.637.0",
        "@commitlint/cli": "19.4.1",
        "@commitlint/config-conventional": "19.4.1",
        "@dffp/clash-discord-bot-cli": "workspace:*",
        "@eslint/js": "9.9.1",
        "@stylistic/eslint-plugin": "2.7.1",
        "@types/aws-lambda": "8.10.145",
        "@types/eslint__js": "8.42.3",
        "@types/node": "22.5.1",
        "@types/string-similarity": "4.0.2",
        "@types/uuid": "10.0.0",
        "@vitest/coverage-istanbul": "2.0.5",
        "discord-api-types": "0.37.98",
        "esbuild": "0.23.1",
        "eslint": "9.9.1",
        "husky": "9.1.5",
        "tsx": "4.19.0",
        "typescript": "5.5.4",
        "typescript-eslint": "8.3.0",
        "vite": "5.4.2",
        "vite-tsconfig-paths": "5.0.1",
        "vitest": "2.0.5"
    }
}
