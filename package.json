{
  "name": "clash-discord-bot",
  "version": "1.0.0",
  "description": "",
  "author": "",
  "license": "ISC",
  "type": "module",
  "engineStrict": true,
  "engines": {
    "node": "22.13.0",
    "pnpm": "10.0.0"
  },
  "packageManager": "pnpm@10.0.0",
  "scripts": {
    "proxy": "tsx dev/ws-proxy.ts",
    "dev": "nodemon --exec tsx dev/ws-client.ts",
    "types": "tsc --project tsconfig.check.json --noEmit",
    "build": "rspack build",
    "build:doc": "rspack build --config rspack.config.doctor.mjs",
    "build:dev": "rspack build --config rspack.config.dev.mjs",
    "build:dev:doc": "rspack build --config rspack.config.dev.doctor.mjs",
    "coverage": "vitest --coverage",
    "lint": "eslint . --quiet",
    "fixlint": "eslint . --fix",
    "unit": "echo 'not implemented'",
    "format": "terraform -chdir=terraform fmt -recursive",
    "init": "terraform -chdir=terraform init",
    "select": "terraform -chdir=terraform workspace select",
    "show": "terraform -chdir=terraform workspace show",
    "plan": "pnpm run show && terraform -chdir=terraform plan -var-file .secret.tfvars -out .plan.tfplan",
    "apply": "terraform -chdir=terraform apply .plan.tfplan",
    "reset": "rm -rf **/node_modules **/dist **/*.tfplan **/*.lock.hcl **/.terraform",
    "sendit": "pnpm run build && pnpm run plan && pnpm run apply",
    "teehee": "export BUILD_ENV=$(terraform -chdir=terraform workspace show) && pnpm run format && pnpm run types && pnpm run sendit",
    "commitlint": "commitlint --edit",
    "slash": "tsx scripts/slash.ts",
    "prepare": "husky"
  },
  "dependencies": {
    "@effect-aws/client-api-gateway-management-api": "1.6.0",
    "@effect-aws/client-dynamodb": "1.7.0",
    "@effect-aws/client-eventbridge": "1.8.0",
    "@effect-aws/client-lambda": "1.7.0",
    "@effect-aws/client-scheduler": "1.2.0",
    "@effect-aws/client-sqs": "1.7.0",
    "@effect-aws/client-ssm": "1.4.0",
    "@effect-aws/lambda": "1.2.0",
    "@effect-aws/lib-dynamodb": "1.4.1",
    "@effect-aws/ssm": "1.0.2",
    "@effect/platform": "0.75.2",
    "@effect/platform-node": "0.71.2",
    "@hapi/boom": "10.0.1",
    "@hapi/bourne": "3.0.0",
    "clashofclans.js": "3.3.20",
    "dayjs": "1.11.13",
    "dfx": "0.109.0",
    "discord-interactions": "4.1.1",
    "effect": "3.12.8",
    "joi": "17.13.3",
    "regexparam": "3.0.0",
    "simple-statistics": "7.8.7",
    "string-similarity": "4.0.4",
    "uuid": "11.0.5"
  },
  "devDependencies": {
    "@changesets/cli": "2.27.12",
    "@commitlint/cli": "19.7.1",
    "@commitlint/config-conventional": "19.7.1",
    "@discordjs/core": "2.0.1",
    "@effect/vitest": "0.17.4",
    "@eslint/js": "9.19.0",
    "@hapi/hapi": "21.3.12",
    "@hapi/nes": "14.0.1",
    "@rsdoctor/cli": "0.4.13",
    "@rsdoctor/rspack-plugin": "0.4.13",
    "@rspack/cli": "1.2.2",
    "@rspack/core": "1.2.2",
    "@stylistic/eslint-plugin": "3.0.1",
    "@types/aws-lambda": "8.10.147",
    "@types/eslint__js": "8.42.3",
    "@types/string-similarity": "4.0.2",
    "@types/uuid": "10.0.0",
    "@types/ws": "8.5.14",
    "@vitest/coverage-istanbul": "3.0.5",
    "esbuild": "0.24.2",
    "esbuild-loader": "4.2.2",
    "esbuild-register": "3.6.0",
    "eslint": "9.19.0",
    "husky": "9.1.7",
    "nodemon": "3.1.9",
    "terser-webpack-plugin": "5.3.11",
    "ts-node": "10.9.2",
    "tsx": "4.19.2",
    "typescript": "5.7.3",
    "typescript-eslint": "8.23.0",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.0.5",
    "ws": "8.18.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild",
      "@parcel/watcher"
    ]
  }
}
