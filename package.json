{
  "name": "clash-discord-bot",
  "version": "1.0.0",
  "description": "",
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.9.0",
  "engineStrict": true,
  "engines": {
    "node": "22.15.0"
  },
  "type": "module",
  "scripts": {
    "pnpmv": "pnpm -v",
    "bench": "vitest bench",
    "lsp": "typescript-language-server --stdio",
    "merge": "pnpm i --merge-git-branch-lockfiles",
    "proxy": "tsx dev/ws-proxy.ts",
    "dev": "nodemon --exec tsx dev/ws-client.ts",
    "devrun": "tsx dev/run.ts",
    "types": "tsc --project tsconfig.check.json --noEmit",
    "build": "rspack build",
    "build:doc": "rspack build --config rspack.config.doctor.mjs",
    "build:dev": "rspack build --config rspack.config.dev.mjs",
    "build:dev:doc": "rspack build --config rspack.config.dev.doctor.mjs",
    "coverage": "vitest --coverage",
    "lint": "eslint . --quiet",
    "fixlint": "eslint . --fix",
    "unit": "vitest test/unit",
    "format": "terraform -chdir=terraform fmt -recursive",
    "init": "terraform -chdir=terraform init",
    "select": "terraform -chdir=terraform workspace select",
    "show": "terraform -chdir=terraform workspace show",
    "plan": "pnpm run show && terraform -chdir=terraform plan -var-file .secret.tfvars -out .plan.tfplan",
    "apply": "terraform -chdir=terraform apply .plan.tfplan",
    "reset": "rm -rf **/node_modules **/dist **/*.tfplan **/*.lock.hcl **/.terraform",
    "sendit": "pnpm run build && pnpm run plan && pnpm run apply",
    "teehee": "export BUILD_ENV=$(terraform -chdir=terraform workspace show) && pnpm run format && pnpm run types && pnpm run sendit",
    "commitlint": "commitlint --edit",
    "slash": "tsx scripts/slash.ts",
    "prepare": "husky"
  },
  "dependencies": {
    "@effect-aws/client-api-gateway-management-api": "1.10.0",
    "@effect-aws/client-dynamodb": "1.10.0",
    "@effect-aws/client-eventbridge": "1.10.0",
    "@effect-aws/client-lambda": "1.10.0",
    "@effect-aws/client-scheduler": "1.10.0",
    "@effect-aws/client-sqs": "1.10.0",
    "@effect-aws/lambda": "1.3.1",
    "@effect-aws/lib-dynamodb": "1.10.0",
    "@effect-aws/ssm": "1.3.0",
    "@effect/platform-node": "0.77.2",
    "@msgpack/msgpack": "3.1.1",
    "clashofclans.js": "3.3.22",
    "dayjs": "1.11.13",
    "dfx": "0.115.0",
    "discord-verify": "1.2.0",
    "effect": "3.14.14",
    "pako": "2.1.0",
    "regexparam": "3.0.0",
    "simple-statistics": "7.8.8",
    "string-similarity": "4.0.4",
    "uuid": "11.1.0"
  },
  "devDependencies": {
    "@aws-sdk/lib-dynamodb": "3.797.0",
    "@changesets/cli": "2.29.2",
    "@commitlint/cli": "19.8.0",
    "@commitlint/config-conventional": "19.8.0",
    "@discordjs/core": "2.1.0",
    "@effect/language-service": "0.8.1",
    "@effect/vitest": "0.20.15",
    "@eslint/js": "9.25.1",
    "@hapi/hapi": "21.4.0",
    "@hapi/nes": "14.0.1",
    "@rsdoctor/cli": "1.0.2",
    "@rsdoctor/rspack-plugin": "1.0.2",
    "@rspack/cli": "1.3.7",
    "@rspack/core": "1.3.7",
    "@stylistic/eslint-plugin": "4.2.0",
    "@tim-smart/openapi-gen": "0.3.11",
    "@types/aws-lambda": "8.10.149",
    "@types/pako": "2.0.3",
    "@types/string-similarity": "4.0.2",
    "@types/uuid": "10.0.0",
    "@types/ws": "8.18.1",
    "@vitest/coverage-v8": "3.1.2",
    "esbuild": "0.25.3",
    "eslint": "9.25.1",
    "husky": "9.1.7",
    "nodemon": "3.1.10",
    "openapi-typescript": "7.6.1",
    "safe-json-stringify": "1.2.0",
    "tsoa": "6.6.0",
    "tsx": "4.19.3",
    "typescript": "5.8.3",
    "typescript-eslint": "8.31.0",
    "typescript-language-server": "4.3.4",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.1.2",
    "ws": "8.18.1"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild",
      "@parcel/watcher"
    ],
    "ignoredBuiltDependencies": [
      "msgpackr-extract"
    ]
  }
}
